				id:laws
				source:"eng/icons/laws0.png"
				on_release:
					app.pageinfo()
				on_press:
					app.Klik()	
			ImageButton:
				id:zvuk8				
				source:"eng/icons/voloff.png"
				on_press:
					app.Klik()	
				on_release:
					app.Zvuk()
			
			ImageButton:
				source:"eng/icons/language.png"
				on_press:
					app.Klik()
			ImageButton:
				source:"eng/icons/help.png"
				on_press:
