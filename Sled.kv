<Label>:
	font_name:"style.ttf"
<Sled>:
	FloatLayout:
		canvas.before:
			Rectangle:
				pos:self.pos
				size:self.size			
				source:"eng/backfon/nextone.png"
		Label:
			id:teper_hodit
			font_size:50
			bold:True
			color:[0,0,0,1]
			pos_hint:{"top":.32,"right":.7}
			size_hint:.5,.1
			on_touch_down:
				app.change_screen("vopros_screen")
				app.Klik()
		
		